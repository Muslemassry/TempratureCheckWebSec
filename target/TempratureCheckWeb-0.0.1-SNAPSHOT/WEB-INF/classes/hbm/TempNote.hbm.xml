<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
<class name="com.temp.model.TempNote" table="TEMP_NOTE">
	<id name="tempNoteId" 								column="TEMP_NOTE_ID">
		<generator class="increment" />
	</id>
	<property name="noteDesc" 		type="string" 		column="NOTE_DESC"/>
	<property name="degree" 		type="integer" 		column="DEGREE"/>
	<property name="day" 			type="timestamp" 	column="DAY"/>
	<property name="dateCreated" 	type="timestamp" 	column="DATE_CREATED"/>
	<property name="dateLastUpdate" type="timestamp" 	column="DATE_LAST_UPDATE"/>
	<property name="userCreated" 	type="string" 		column="USER_CREATED"/>
	<property name="userLastUpdate" type="string" 		column="USER_LAST_UPDATE"/>
</class>
</hibernate-mapping>
