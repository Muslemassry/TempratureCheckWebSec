<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
<class name="com.temp.model.Person" table="PERSON">
	<id name="personNumber" column="PERSON_NUMBER">
		<generator class="increment" />
	</id>
	<property name="email" 			type="string" column="EMAIL"/>
	<property name="phone" 			type="string" column="PHONE"/>
	<property name="username" 		type="string" column="USERNAME"/>
	<property name="password" 		type="string" column="UPASSWORD"/>
	<property name="admin" 			type="string" column="IS_Admin"/>
	<property name="dateCreated" 	type="timestamp" column="DATE_CREATED"/>
	<property name="dateLastUpdate" type="timestamp" column="DATE_LAST_UPDATE"/>
	<property name="userCreated" 	type="string" column="USER_CREATED"/>
	<property name="userLastUpdate" type="string" column="USER_LAST_UPDATE"/>
</class>
</hibernate-mapping>
